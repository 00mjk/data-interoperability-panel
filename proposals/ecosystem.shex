PREFIX ecosystem: <http://www.w3.org/ns/solid/ecosystem#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

################################################################
# Application Registries
################################################################

# Registrar
<#RegistrarShape> {
  a [ ecosystem:Registrar ] ;
  ecosystem:hasApplicationRegistrySet IRI ;
  ecosystem:hasDataRegistrySet IRI
}

# Application Registry Set
################################################################
<#ApplicationRegistrySetShape> {
  a [ ecosystem:ApplicationRegistrySet ] ;
  ecosystem:hasRegistry IRI+
}

# Application Registry
################################################################
<#ApplicationRegistryShape> {
  a [ ecosystem:ApplicationRegistry ] ;
  ecosystem:hasRegistration IRI*
}

# Application Registration
################################################################
<#ApplicationRegistrationShape> {
  a [ ecosystem:ApplicationRegistration ] ;
  ecosystem:registeredBy IRI ;
  ecosystem:registeredWith IRI ;
  ecosystem:registeredApplication IRI ;
  ecosystem:registeredAt xsd:dateTime
}

################################################################
# Data Registries
################################################################

# Data Registry Set
################################################################
<#DataRegistrySetShape> {
  a [ ecosystem:DataRegistrySet ] ;
  ecosystem:hasRegistry IRI+
}

# Data Registry
################################################################
<#DataRegistryShape> {
  a [ ecosystem:DataRegistry ] ;
  ecosystem:hasRegistration IRI*
}
