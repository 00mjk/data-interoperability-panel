@prefix : <#> .
@prefix solid: <http://www.w3.org/ns/solid/terms> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<>
dcterms:issued "2020-06-14"^^xsd:date ;
dcterms:modified "2020-06-14"^^xsd:date ;
rdfs:label "Solid ecosystem terms"@en .

# Agent Class
#############################################################################
# Related to a human, software, or device agent
# participating in the Solid Ecosystem
#############################################################################
:Agent
  a rdfs:Class ;
  rdfs:comment "An agent profile for the Solid Ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Agent Profile"@en .

# Properties in domain of Agent
#############################################################################
:hasRegistrySet
  a rdf:property ;
  rdfs:comment "Links an agent's profile to a registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :RegistrySet ;
  rdfs:label "Has registry set"@en .

:hasApplicationRegistrySet
  a rdf:property ;
  rdfs:comment "Links an agent's profile to an application registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :ApplicationRegistrySet ;
  rdfs:label "Has application registry set"@en .

:hasDataRegistrySet
  a rdf:property ;
  rdfs:comment "Links an agent's profile to a data registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :DataRegistrySet ;
  rdfs:label "Has data registry set"@en .

:trustsForApplicationRegistration
  a rdf:property ;
  rdfs:comment "Links to an application the agent trusts to assist with registering applications"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :Application ;
  rdfs:label "Trusts for application registration"@en .

# RegistrySet Class
#############################################################################
# Used by an Agent to organize a collection of Registry instances
#############################################################################
:RegistrySet
  a rdfs:Class ;
  rdfs:comment "A collection of Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registry Set"@en .

# Properties in domain of RegistrySet
#############################################################################
:hasRegistry
  a rdf:property ;
  rdfs:comment "Links to a registry in a registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :RegistrySet ;
  rdfs:range :Registry ;
  rdfs:label "Has registry"@en .

# ApplicationRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for ApplicationRegistry Instances
#############################################################################
:ApplicationRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Application Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registry"@en .

# DataRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for DataRegistry Instances
#############################################################################
:DataRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Data Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Data Registry"@en .

# Registry Class
#############################################################################
# Used by an Agent to organize collections of things necessary to
# participte in the Solid ecosystem
#############################################################################
:Registry
  a rdfs:Class ;
  rdfs:comment "A collection of things pertinent to the operation of the Solid ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registry"@en .

# Properties in domain of Registry
#############################################################################
:hasRegistration
  a rdf:property ;
  rdfs:comment "Links to a registration in a given registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registry ;
  rdfs:range :Registration ;
  rdfs:label "Has registration"@en .

# ApplicationRegistry Class
#############################################################################
# SubClassOf Registry used for ApplicationRegistration instances
#############################################################################
:ApplicationRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Application Registration instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registry"@en .

# DataRegistry Class
#############################################################################
# SubClassOf Registry used for DataRegistration instances
#############################################################################
:DataRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Data Registration instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Data Registry"@en .

# Registration Class
#############################################################################
# A Registration is an individual entry in a Registry
#############################################################################
:Registration
  a rdfs:Class ;
  rdfs:comment "An individual member of a Registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registration"@en .

# Properties in domain of Registration
#############################################################################
:registeredBy
  a rdf:property ;
  rdfs:comment "Links to the Agent that created the registration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range :Agent ;
  rdfs:label "Registered By"@en .

:registeredWith
  a rdf:property ;
  rdfs:comment "Links to the client Application that was used to create the registration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range :Application ;
  rdfs:label "Registered With"@en .

:registeredAt
  a rdf:property ;
  rdfs:comment "Data and time the registration was created"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Registered At"@en .

# ApplicationRegistration Class
#############################################################################
# subClassof Registration used for entries in an ApplicationRegistry
#############################################################################
:ApplicationRegistration
  rdfs:subClassOf :Registration ;
  rdfs:comment "An individual member of an Application Registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registration"@en .

# Properties in domain of ApplicationRegistration
#############################################################################
:registeredApplication
  a rdf:property ;
  rdfs:comment "Links to the Application registered by an ApplicationRegistration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :ApplicationRegistration ;
  rdfs:range :Application ;
  rdfs:label "Registered Application"@en .

# Application Class
#############################################################################
# Related to a client software application of any kind that
# participates in the Solid ecosystem
#############################################################################
:Application
  a rdfs:Class ;
  rdfs:comment "A client software application in the Solid Ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application"@en .
