@prefix : <#> .
@prefix solid: <http://www.w3.org/ns/solid/terms> .
@prefix st: <http://www.w3.org/ns/shapetree#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<>
dcterms:issued "2020-06-14"^^xsd:date ;
dcterms:modified "2020-06-14"^^xsd:date ;
rdfs:label "Solid ecosystem terms"@en .

# Agent Class
#############################################################################
# Related to a human, software, or device agent
# participating in the Solid Ecosystem
#############################################################################
:Agent
  a rdfs:Class ;
  rdfs:comment "An agent profile for the Solid Ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Agent"@en .

# Properties in domain of Agent
#########################################################
:hasRegistrar
  a rdf:property ;
  rdfs:comment "Links an Agent to a registrar"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :Registrar ;
  rdfs:label "Has registrar"@en .

:hasAccessInbox
  a rdf:property ;
  rdfs:comment "Links an Agent to an Access inbox for Access Receipts"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range IRI ;
  rdfs:label "Has Access Inbox"@en .

:trustsApplication
  a rdf:property ;
  rdfs:comment "Links to an Application the Agent trusts"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :Application ;
  rdfs:label "Trusts application"@en .

:trustsForApplicationRegistration
  rdfs:subPropertyOf :trustsApplication ;
  rdfs:comment "Links to an application the agent trusts to assist with registering applications"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :Application ;
  rdfs:label "Trusts for application registration"@en .

:trustsForDataAuthorization
  rdfs:subPropertyOf :trustsApplication ;
  rdfs:comment "Links to an application the agent trusts to assist with registering and authorizing access to data"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Agent ;
  rdfs:range :Application ;
  rdfs:label "Trusts for data registration and authorization"@en .

# Registrar
#############################################################################
# Used by an Agent to organize a collection of Registry Sets
#############################################################################
:Registrar
  a rdfs:Class ;
  rdfs:comment "A collection of an Agent's registry sets"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registrar"@en .

# Properties in domain of Registrar
#########################################################
:hasRegistrySet
  a rdf:property ;
  rdfs:comment "Links an agent's registrar to a registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registrar ;
  rdfs:range :RegistrySet ;
  rdfs:label "Has registry set"@en .

:hasApplicationRegistrySet
  rdfs:subPropertyOf :hasRegistrySet ;
  rdfs:comment "Links an agent's registrar to an application registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registrar ;
  rdfs:range :ApplicationRegistrySet ;
  rdfs:label "Has application registry set"@en .

:hasDataRegistrySet
  rdfs:subPropertyOf :hasRegistrySet ;
  rdfs:comment "Links an agent's registrar to a data registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registrar ;
  rdfs:range :DataRegistrySet ;
  rdfs:label "Has data registry set"@en .

:hasAccessRegistrySet
  rdfs:subPropertyOf :hasRegistrySet ;
  rdfs:comment "Links an agent's registrar to an access registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registrar ;
  rdfs:range :AccessRegistrySet ;
  rdfs:label "Has access registry set"@en .

# RegistrySet Class
#############################################################################
# Used by an Agent to organize a collection of Registry instances
#############################################################################
:RegistrySet
  a rdfs:Class ;
  rdfs:comment "A collection of Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registry Set"@en .

# Properties in domain of RegistrySet
#########################################################
:hasRegistry
  a rdf:property ;
  rdfs:comment "Links to a registry in a registry set"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :RegistrySet ;
  rdfs:range :Registry ;
  rdfs:label "Has registry"@en .

# ApplicationRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for ApplicationRegistry Instances
#############################################################################
:ApplicationRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Application Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registry"@en .

# DataRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for DataRegistry Instances
#############################################################################
:DataRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Data Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Data Registry"@en .

# AccessRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for AccessRegistry Instances
#############################################################################
:AccessRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Access Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Registry Set"@en .

# AccessReceiptRegistrySet Class
#############################################################################
# SubClassOf RegistrySet used for AccessReceiptRegistry Instances
#############################################################################
:AccessReceiptRegistrySet
  rdfs:subClassOf :RegistrySet ;
  rdfs:comment "A collection of Access Receipt Registry instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Receipt Registry Set"@en .

# Registry Class
#############################################################################
# Used by an Agent to organize collections of things necessary to
# participte in the Solid ecosystem
#############################################################################
:Registry
  a rdfs:Class ;
  rdfs:comment "A collection of things pertinent to the operation of the Solid ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registry"@en .

# Properties in domain of Registry
#########################################################
:hasRegistration
  a rdf:property ;
  rdfs:comment "Links to a registration in a given registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registry ;
  rdfs:range :Registration ;
  rdfs:label "Has registration"@en .

# ApplicationRegistry Class
#############################################################################
# SubClassOf Registry used for ApplicationRegistration instances
#############################################################################
:ApplicationRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Application Registration instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registry"@en .

# DataRegistry Class
#############################################################################
# SubClassOf Registry used for DataRegistration instances
#############################################################################
:DataRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Data Registration instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Data Registry"@en .

# AccessRegistry Class
#############################################################################
# SubClassOf Registry which stores registered AccessGrants
#############################################################################
:AccessRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Access Grant instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Registry"@en .

# AccessReceiptRegistry Class
#############################################################################
# SubClassOf Registry which stores registered AccessReceipts
#############################################################################
:AccessReceiptRegistry
  rdfs:subClassOf :Registry ;
  rdfs:comment "A collection of Access Receipt instances"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Receipt Registry"@en .

# Registration Class
#############################################################################
# A Registration is an individual entry in a Registry
#############################################################################
:Registration
  a rdfs:Class ;
  rdfs:comment "An individual member of a Registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registration"@en .

# Properties in domain of Registration
#########################################################
:registeredBy
  a rdf:property ;
  rdfs:comment "Links to the Agent that created the registration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range :Agent ;
  rdfs:label "Registered By"@en .

:registeredWith
  a rdf:property ;
  rdfs:comment "Links to the client Application that was used to create the registration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range :Application ;
  rdfs:label "Registered With"@en .

:registeredAt
  a rdf:property ;
  rdfs:comment "Data and time the registration was created"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Registered At"@en .

:updatedAt
  a rdf:property ;
  rdfs:comment "Date and time the registration was updated"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Registration ;
  rdfs:range xsd:dateTime ;
  rdfs:label "Updated At"@en .

# ApplicationRegistration Class
#############################################################################
# subClassof Registration used for entries in an ApplicationRegistry
#############################################################################
:ApplicationRegistration
  rdfs:subClassOf :Registration ;
  rdfs:comment "An individual member of an Application Registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application Registration"@en .

# Properties in domain of ApplicationRegistration
#########################################################
:registeredApplication
  a rdf:property ;
  rdfs:comment "Links to the Application registered by an ApplicationRegistration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :ApplicationRegistration ;
  rdfs:range :Application ;
  rdfs:label "Registered Application"@en .

# DataRegistration Class
#############################################################################
# subClassof Registration used for entries in a DataRegistry
#############################################################################
:DataRegistration
  rdfs:subClassOf :Registration ;
  rdfs:comment "An individual member of a Data Registry"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Data Registration"@en .

# Properties in domain of DataRegistration
#########################################################
:registeredShapeTree
  a rdf:property ;
  rdfs:comment "Links to the shape tree associated with a given Data Registration"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range st:ShapeTree ;
  rdfs:label "Registered Shape Tree"@en .

# No domain is provided because this is also used by other classes in the
# ecosystem (e.g. AccessNeed). No range is provided because it could point
# to any kind of registered data.
:hasRegisteredDataInstance
  a rdf:property ;
  rdfs:comment "Links to instances of the registered shape tree"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Registered Data Instance"@en .

# Application Class
#############################################################################
# Related to a client software application of any kind that
# participates in the Solid ecosystem
#############################################################################
:Application
  a rdfs:Class ;
  rdfs:comment "A client software application in the Solid Ecosystem"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Application"@en .

# Properties in domain of Application
#########################################################
:applicationName
  a rdf:property ;
  rdfs:comment "Name of the Application"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:range xsd:string ;
  rdfs:label "Application Name"@en .

:applicationDescription
  a rdf:property ;
  rdfs:comment "Description of the Application"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:range xsd:string ;
  rdfs:label "Application Description"@en .

:applicationAuthor
  a rdf:property ;
  rdfs:comment "Author of the Application"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:range :Agent ;
  rdfs:label "Application Author"@en .

:applicationAuthorName
  a rdf:property ;
  rdfs:comment "Name of Application Author"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:range xsd:string ;
  rdfs:label "Application Author Name"@en .

:applicationThumbnail
  a rdf:property ;
  rdfs:comment "Thumbnail for the application"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:label "Application Thumbnail"@en .

:hasAccessNeedGroup
  a rdf:property ;
  rdfs:comment "Links to AccessNeedGroup"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range :AccessNeedGroup ;
  rdfs:label "Has Access Need Group"@en .

:hasCollaborativeAccessNeedGroup
  a rdf:property ;
  rdfs:comment "Links to AccessNeedGroup used for data sharing"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :Application ;
  rdfs:range :AccessNeedGroup ;
  rdfs:label "Has Collaborative Access Need Group"@en .

# AccessNeedGroup Class
#############################################################################
# Related to a group of AccessNeeds communicated to a given Agent
# in the ecosystem by an Application or other Agent
#############################################################################
:AccessNeedGroup
  a rdfs:Class ;
  rdfs:comment "A grouping of access needs"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Need Group"@en .

# Properties in domain of AccessNeedGroup
#########################################################
:hasAccessNeed
  a rdf:property ;
  rdfs:comment "Links to an Access Need"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range :AccessNeed ;
  rdfs:label "Has Access Need"@en .

:hasAccessNeedOverride
  a rdf:property ;
  rdfs:comment "Overrides inherited evaluation of an Access Need"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range :AccessNeed ;
  rdfs:label "Has Access Need Override"@en .

:hasAccessNeedDecoratorIndex
  a rdf:property ;
  rdfs:comment "SKOS Index for describing Access Needs"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :AccessNeedGroup ;
  rdfs:label "Access Decorator Index"@en .

:authenticatesAs
  a rdf:property ;
  rdfs:comment "Agent or mode the Application authenticates as"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :AccessNeedGroup ;
  rdfs:label "Authenticates As"@en .

:isEnabled
  a rdf:property ;
  rdfs:comment "Whether the group has been enabled by the Agent"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :AccessNeedGroup ;
  rdfs:range xsd:boolean ;
  rdfs:label "Is Enabled"@en .

# IRI that represents a piloted agent
#############################################################################
:Pilot
  rdfs:comment "Authenticates as Agent piloting the Application"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Pilot"@en .

# AccessNeed Class
#############################################################################
# Identifies the type of data and access level need for that data
#############################################################################
:AccessNeed
a rdfs:Class ;
rdfs:comment "Request for access to a type of data at a specified access level"@en ;
rdfs:isDefinedBy <> ;
rdfs:label "Access Need"@en .

# Properties in domain of AccessNeed
#########################################################
:inAccessNeedGroup
a rdf:property ;
rdfs:comment "Identifies the AccessNeedGroup the AccessNeed belongs to"@en ;
rdfs:isDefinedBy <> ;
rdfs:domain :AccessNeed ;
rdfs:range :AccessNeedGroup ;
rdfs:label "In Access Need Group"@en .

:recursivelyAuthorize
a rdf:property ;
xrdfs:comment "Continue traversing shape tree hierarchy for authorization when true"@en ;
rdfs:isDefinedBy <> ;
rdfs:domain :AccessNeed ;
rdfs:range xsd:boolean ;
rdfs:label "Recursively Authorize"@en .

:accessNecessity
a rdf:property ;
rdfs:comment "Identifies the necessity of the AccessNeed"@en ;
rdfs:isDefinedBy <> ;
rdfs:range [ owl:oneOf ( :AccessRequired :AccessOptional ) ] ;
rdfs:label "Requested Access Necessity"@en .

:accessScenario
  a rdf:property ;
  rdfs:comment "Provides context types for sharing scenarios"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range [ owl:oneOf ( :PersonalAccess :SharedAccess ) ] ;
  rdfs:label "Access Scenario"@en .

:accessMode
a rdf:property ;
rdfs:comment "A requested acl:mode of access for an AccessNeed"@en ;
rdfs:isDefinedBy <> ;
rdfs:range [ owl:oneOf ( acl:Read acl:Write acl:Control acl:Append ) ] ;
rdfs:label "Requested Access Mode"@en .

# IRIs for accessNecessity types
#############################################################################
:AccessRequired
  rdfs:comment "AccessNeed is required"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Required"@en .

:AccessOptional
  rdfs:comment "AccessNeed is not required"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Optional"@en .

# IRIs for accessScenario types
#############################################################################
:PersonalAccess
  rdfs:comment "Access is for personal use of the Agent"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Personal Access"@en .

:SharedAccess
  rdfs:comment "Access is to be shared with other Agents"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Shared Access"@en .

# PreparedAccessNeedGroup Class
#############################################################################
# Result of an AccessNeedGroup whose AccessNeeds have been processed and
# prepared for evaluation by the Agent in control of the data
#############################################################################
:PreparedAccessNeedGroup
  a rdfs:Class ;
  rdfs:comment "Group of access needs prepared for evaluation"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Need"@en .

:fromAccessNeedGroup
  a rdf:property ;
  rdfs:comment "Links to the AccessNeedGroup used to generate the PreparedAccessNeedGroup"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeedGroup ;
  rdfs:range :AccessNeedGroup ;
  rdfs:label "Has Access Need Group"@en .

:hasAccessNeedGroupDecorator
  a rdf:property ;
  rdfs:comment "Links to an AccessNeedGroupDecorator"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeedGroup ;
  rdfs:range :AccessNeedGroupDecorator ;
  rdfs:label "Has Access Need Group Decorator"@en .

:hasPreparedAccessNeed
  a rdf:property ;
  rdfs:comment "Links to a PreparedAccessNeed"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeedGroup ;
  rdfs:range :AccessNeedGroupDecorator ;
  rdfs:label "Has Access Need Group Decorator"@en .

# PreparedAccessNeed Class
#############################################################################
# Result of an AccessNeed that has been prepared for evaluation by
# the Agent in control of the data
#############################################################################
:PreparedAccessNeed
  a rdfs:Class ;
  rdfs:comment "Access need that have been prepared for evaluation"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Need"@en .

:inPreparedAccessNeedGroup
  a rdf:property ;
  rdfs:comment "Identifies the PreparedAccessNeedGroup the PreparedAccessNeed belongs to"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range :PreparedAccessNeedGroup ;
  rdfs:label "In Prepared Access Need Group"@en .

:supportsPreparedAccessNeed
  a rdf:property ;
  rdfs:comment "Links to a PreparedAccessNeed this PreparedAccessNeed supports"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range :PreparedAccessNeed ;
  rdfs:label "Supports Prepared Access Need"@en .

:supportedByPreparedAccessNeed
  a rdf:property ;
  rdfs:comment "Links to a PreparedAccessNeed supported by this PreparedAccessNeed"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range :PreparedAccessNeed ;
  rdfs:label "Supported By Prepared Access Need"@en .

:hasShapeTreeDecorator
  a rdf:property ;
  rdfs:comment "Links to a ShapeTreeDecorator"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range st:ShapeTreeDecorator ;
  rdfs:label "Has Shape Tree Decorator"@en .

:hasAccessNeedDecorator
  a rdf:property ;
  rdfs:comment "Links to an AccessNeedDecorator"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range :AccessNeedDecorator ;
  rdfs:label "Has Access Need Decorator"@en .

:referencesPreparedAccessNeed
  a rdf:property ;
  rdfs:comment "References another PreparedAccessNeed"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:domain :PreparedAccessNeed ;
  rdfs:range :PreparedAccessNeed ;
  rdfs:label "References Prepared Access Need"@en .

:hasDataGrant
  a rdf:property ;
  rdfs:comment "Links to a DataGrant"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:range :DataGrant ;
  rdfs:label "Has Data Grant"@en .

# AccessDecoratorIndex class
#############################################################################
# An AccessDecoratorIndex details AccessNeeds and AccessNeedGroups
# in a human-readable way for the purposes of authorization.
#############################################################################
:AccessNeedDecoratorIndex
  a rdfs:Class ;
  rdfs:comment "Links an index of SKOS poly-hierarchies to describe AccessNeeds in an AccessNeedGroup"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "AccessNeed Decorator Index"@en .

# Properties in domain of AccessDecoratorIndex
#############################################################################
:hasSeries
  a rdf:property ;
  rdfs:domain :AccessDecoratorIndex ;
  rdfs:range :AccessDecoratorSeries ;
  rdfs:comment "Links an AccessDecoratorIndex to a AccessDecoratorSeries"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Has Series"@en .

:defaultLanguage
  a rdf:property ;
  rdfs:domain :AccessDecoratorIndex ;
  rdfs:range xsd:lang ;
  rdfs:comment "Default language code"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Default Language"@en .

 # AccessDecoratorSeries class
#############################################################################
# Details a versioned set of AccessDecoratorVersion classes
# Primary use is grouping AccessDecoratorVersion instances by a common
# discriminator such as language or intended audience
#############################################################################
:AccessDecoratorSeries
  a rdfs:Class ;
  rdfs:comment "Groups a set of AccessDecoratorVersion by a given discriminator"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "AccessNeed Decorator Series"@en .

# Properties in domain of AccessDecoratorSeries
#############################################################################
:hasVersion
  a rdf:property ;
  rdfs:domain :AccessDecoratorSeries ;
  rdfs:range :AccessDecoratorVersion ;
  rdfs:comment "Links an AccessDecoratorSeries to a AccessDecoratorVersion"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Has Version"@en .

# Properties in domain of AccessDecoratorSeries
#############################################################################
:usesLanguage
  a rdf:property ;
  rdfs:domain :AccessDecoratorSeries ;
  rdfs:range xsd:lang ;
  rdfs:comment "Language code in use"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Uses Language"@en .

# AccessDecoratorVersion class
#############################################################################
# Details a versioned SKOS poly-hierarchy that describes
# an AccessNeedGroup in a given language or format for an intended audience
#############################################################################
:AccessDecoratorVersion
  a rdfs:Class ;
  rdfs:comment "Describes a SKOS poly-hierarchy for purproses of describing AccessNeeds in an AccessNeedGroup"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "AccessNeed Decorator Version"@en .

# Properties in domain of AccessDecoratorVersion
#########################################################
:isVersion
  a rdf:property ;
  rdfs:domain :AccessDecoratorVersion ;
  rdfs:range xsd:string ;
  rdfs:comment "Describes the semantic version of a AccessDecoratorVersion"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Is Version"@en .

:hasSHA256Hash
  a rdf:property ;
  rdfs:domain :AccessDecoratorVersion ;
  rdfs:range xsd:string ;
  rdfs:comment "Describes the SHA-256 of a AccessDecoratorVersion"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Has SHA-256 Hash"@en .

:hasAccessDecoratorResource
  a rdf:property ;
  rdfs:domain :AccessDecoratorVersion ;
  rdfs:comment "Represents the IRI to the SKOS graph described by AccessDecoratorVersion instance"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Has Decorator Resource"@en .

# AccessDecoratorResource class
#############################################################################
# A resource containing AccessDecorators
#############################################################################
:AccessDecoratorResource
  a rdfs:Class ;
  rdfs:comment "Resource containing Access Decorators"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Decorator Resource"@en .

# AccessDecorator class
#############################################################################
# Associates a SKOS description with different types of Access
# Needs and Groupings
#############################################################################
:AccessDecorator
  a rdfs:Class ;
  rdfs:comment "Describes the association between a SKOS label and an Access Need"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "Access Decorator"@en .

# AccessNeedDecorator class
#############################################################################
# Associates a SKOS description with an AccessNeed to describe why that
# access is needed
#############################################################################
:AccessNeedDecorator
  rdfs:subClassOf :AccessDecorator ;
  rdfs:comment "Describes the association between a SKOS label and an Access Need"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "AccessNeed Decorator"@en .

# AccessNeedGroupDecorator class
#############################################################################
# Associates a SKOS description with an AccessNeedGroup to describe what
# is the overall purpose of the access request in the AccessNeedGroup
#############################################################################
:AccessNeedGroupDecorator
  rdfs:subClassOf :AccessDecorator ;
  rdfs:comment "Describes the association between a SKOS label and an Access Need Group"@en ;
  rdfs:isDefinedBy <> ;
  rdfs:label "AccessNeedGroup Decorator"@en .

# AccessGrant Class
#############################################################################
# Records an access decision made by an Agent, stored in the Access Registry
#############################################################################
:AccessGrant
  rdfs:subClassOf :Registration
  rdfs:comment "Records an access grant made by an Agent to another Agent for data in their control"@en ;
